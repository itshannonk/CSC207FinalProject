<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scotiabankpaymentsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".cocacola.clickCustomers"
            android:parentActivityName=".cocacola.home.CCHomeActivity"></activity>
        <activity
            android:name=".businessowner.ClickInvoices"
            android:parentActivityName=".businessowner.home.SBOHomeActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".settings.SettingsActivity" />
        <activity
            android:name=".login.LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".registration.RegisterActivity"
            android:label="@string/app_name"
            android:parentActivityName=".login.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".businessowner.home.SBOHomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".businessowner.SBOSeeOrder"
            android:parentActivityName=".businessowner.ClickInvoices"
            android:screenOrientation="portrait" />
        <activity
            android:name=".businessowner.SBOSettings"
            android:parentActivityName=".businessowner.home.SBOHomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".settings.ChangePassword"
            android:parentActivityName=".businessowner.SBOSettings" />
        <activity
            android:name=".cocacola.home.CCHomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".cocacola.CocaColaSeeInvoices"
            android:parentActivityName=".cocacola.home.CCHomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".cocacola.CocaColaSettings"
            android:parentActivityName=".cocacola.home.CCHomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".driver.home.DriverHomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".driver.DriverSeeOrder"
            android:parentActivityName=".driver.home.DriverHomeActivity" />
        <activity
            android:name=".driver.DriverSetting"
            android:parentActivityName=".driver.home.DriverHomeActivity" />
    </application>

</manifest>